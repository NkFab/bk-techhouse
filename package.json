{
  "name": "new-nodejs-app",
  "version": "1.0.0",
  "description": "NodeJs REST API",
  "main": "app.js",
  "scripts": {
    "build": "npm run clean:dist && npm run build-server",
    "start": "node ./dist",
    "dev": "NODE_ENV=development nodemon --exec babel-node ./src",
    "test": "NODE_ENV=test jest --collect-coverage --detectOpenHandles",
    "coveralls": "NODE_ENV=test jest --coverage && cat ./coverage/lcov.info | coveralls",
    "clean:dist": "rm -rf dist && mkdir dist",
    "build-server": "babel -d ./dist ./src"
  },
  "keywords": [
    "NodeJS",
    "generate-node-project"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "celebrate": "^15.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "husky": "^7.0.1",
    "mongoose": "^5.11.5",
    "mongoose-paginate": "^5.0.3",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "@types/jest": "^26.0.17",
    "babel-jest": "^24.8.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "3.1.1",
    "jest": "^24.8.0",
    "nodemon": "^2.0.0",
    "supertest": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test"
    }
  }
}
